{
	"name": "andreas/prestashop",
	"description": "Prestashop with composer and Laravel components",
    "require": {
        "illuminate/database": "5.*",
        "illuminate/html": "5.*",
        "illuminate/routing": "5.*",
        "illuminate/view": "5.*",
        "illuminate/events": "5.*",
        "illuminate/session": "5.*",
        "illuminate/filesystem": "5.*",
        "illuminate/validation": "5.*",
        "illuminate/translation": "5.*",
        "illuminate/config": "5.*",
        "illuminate/log": "5.*",
        "illuminate/console": "5.*",
        "symfony/var-dumper": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "Illuminato\\": "src/",
            "Illuminato\\Console\\Commands\\": "src/Console/",
            "Illuminate\\Foundation\\": "src/Foundation/",
            "Illuminate\\Foundation\\Providers\\": "src/Foundation/Providers/"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}